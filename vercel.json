{
  "version": 2,
  "name": "spec-workflow-mcp-docs",
  "framework": "vitepress",
  "buildCommand": "pnpm build",
  "outputDirectory": ".vitepress/dist",
  "installCommand": "pnpm install",
  "devCommand": "pnpm dev",
  "public": true,
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "env": {
    "NODE_VERSION": "18"
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}